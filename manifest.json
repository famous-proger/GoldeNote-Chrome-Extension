{
    "name": "GoldeNote",
    "key": "",
    "version": "1.0.0",
    "description": "This extension will help You to save data from Your browser to Google Sheets",
    "icons": {
        "128": "assets/common/img/note128.png",
        "64": "assets/common/img/note64.png",
        "24": "assets/common/img/note24.png",
        "16": "assets/common/img/note16.png"
    },
    "permissions": [
        "identity",
        "storage"
    ],
    "oauth2": {
        "client_id": "364035604785-gm4orr51v0l79kt4fq0s0q8tgqq1i0h7.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/spreadsheets"
        ]
    },
    "browser_action": {
        "default_icon": "assets/common/img/note128.png",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "options_page": "options.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com/; object-src 'self'"
}